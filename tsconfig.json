{
  "compilerOptions": {
    "isolatedModules": true,
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "useDefineForClassFields": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "types": ["node", "vitest/globals"],
    "typeRoots": ["./node_modules/@types", "./types", "./node_modules"],
    "paths": {
      "@data-activation": ["src/modules/data/activation"],
      "@data-app": ["src/modules/data/application"],
      "@data-board": ["src/modules/data/board"],
      "@data-classes": ["src/modules/data/classes"],
      "@data-env": ["src/modules/data/env"],
      "@data-external-tool": ["src/modules/data/external-tool"],
      "@data-file": ["src/modules/data/file"],
      "@data-folder": ["src/modules/data/folder"],
      "@data-group": ["src/modules/data/group"],
      "@data-h5p": ["src/modules/data/h5p"],
      "@data-license": ["src/modules/data/license"],
      "@data-oauth": ["src/modules/data/oauth"],
      "@data-provisioning-options": ["src/modules/data/provisioning-options"],
      "@data-room": ["src/modules/data/room"],
      "@data-school": ["src/modules/data/school"],
      "@data-system": ["src/modules/data/system"],
			"@data-users": ["src/modules/data/users"],
      "@feature-auto-logout": ["src/modules/feature/auto-logout"],
      "@feature-board-file-element": ["src/modules/feature/board-file-element"],
      "@feature-board-submission-element": [
        "src/modules/feature/board-submission-element"
      ],
      "@feature-board-text-element": ["src/modules/feature/board-text-element"],
      "@feature-board-link-element": ["src/modules/feature/board-link-element"],
      "@feature-board-external-tool-element": [
        "src/modules/feature/board-external-tool-element"
      ],
      "@feature-board-drawing-element": [
        "src/modules/feature/board-drawing-element"
      ],
      "@feature-board-collaborative-text-editor-element": [
        "src/modules/feature/board-collaborative-text-editor-element"
      ],
      "@feature-board-video-conference-element": [
        "src/modules/feature/board-video-conference-element"
      ],
      "@feature-board-h5p-element": ["src/modules/feature/board-h5p-element"],
      "@feature-board": ["src/modules/feature/board"],
      "@feature-course-sync": ["src/modules/feature/course-sync"],
      "@feature-editor": ["src/modules/feature/editor"],
      "@feature-render-html": ["src/modules/feature/render-html"],
      "@feature-news": ["src/modules/feature/news"],
      "@feature-media-shelf": ["src/modules/feature/media-shelf"],
      "@feature-board-deleted-element": [
        "src/modules/feature/board-deleted-element"
      ],
      "@feature-room": ["src/modules/feature/room"],
      "@feature-board-folder-element": [
        "src/modules/feature/board-folder-element"
      ],
      "@feature-folder": ["src/modules/feature/folder"],
      "@feature-collabora": ["src/modules/feature/collabora"],
      "@icons/*": ["src/components/icons/*"],
      "@ui-alert": ["src/modules/ui/alert"],
      "@ui-audio-player": ["src/modules/ui/audio-player"],
      "@ui-board": ["src/modules/ui/board"],
      "@ui-chip": ["src/modules/ui/chip"],
      "@ui-confirmation-dialog": ["src/modules/ui/confirmation-dialog"],
      "@ui-controls": ["src/modules/ui/controls"],
      "@ui-data-table": ["src/modules/ui/data-table"],
      "@ui-date-time-picker": ["src/modules/ui/date-time-picker"],
      "@ui-dialog": ["src/modules/ui/dialog"],
      "@ui-extended-icon-btn": ["src/modules/ui/extended-icon-btn"],
      "@ui-kebab-menu": ["src/modules/ui/kebab-menu"],
      "@ui-layout": ["src/modules/ui/layout"],
      "@ui-light-box": ["src/modules/ui/light-box"],
      "@ui-line-clamp": ["src/modules/ui/line-clamp"],
      "@ui-loading-state-dialog": ["src/modules/ui/loading-state-dialog"],
      "@ui-preview-image": ["src/modules/ui/preview-image"],
      "@ui-qr-code": ["src/modules/ui/qr-code"],
      "@ui-room-details": ["src/modules/ui/room-details"],
      "@ui-skip-link": ["src/modules/ui/skip-link"],
      "@ui-speed-dial-menu": ["src/modules/ui/speed-dial-menu"],
      "@ui-video-conference-configuration-dialog": [
        "src/modules/ui/video-conference-configuration-dialog"
      ],
      "@ui-empty-state": ["src/modules/ui/empty-state"],
      "@util-board": ["src/modules/util/board"],
      "@util-validators": ["src/modules/util/validators"],
      "@util-vue": ["src/modules/util/vue"],
      "@util-browser": ["src/modules/util/browser"],
      "@util-sorting": ["src/modules/util/sorting"],
      "@util-input-masks": ["src/modules/util/input-masks"],
      "@util-device-detection": ["src/modules/util/device-detection"],
      "@util-error-notification": ["src/modules/util/error-notification"],
      "@util-error-handling": ["src/modules/util/error-handling"],
      "@util-logger": ["src/modules/util/logger"],
      "@page-board": ["src/modules/page/board"],
      "@page-class-members": ["src/modules/page/class-members"],
      "@page-media-shelf": ["src/modules/page/media-shelf"],
      "@page-fwu-media": ["src/modules/page/fwu-media"],
      "@page-room": ["src/modules/page/room"],
      "@page-folder": ["src/modules/page/folder"],
      "@page-collabora": ["src/modules/page/collabora"],
      "@/*": ["src/*"],
      "@@/*": ["*"]
    },
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"],
    "outDir": "dist"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "tests/**/*.ts",
    "tests/**/*.tsx",
    "types/**/*.d.ts"
  ],
  "exclude": ["node_modules", "dist", "./src/components/base/components.js"]
}
