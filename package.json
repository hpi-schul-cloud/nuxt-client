		"vue-jest": "^3.0.3",
		"vue-style-loader": "^4.1.2",
		"vue-svg-loader": "^0.12.0",
		"vue-template-compiler": "^2.6.10",
		"vuepress": "^1.0.2",
		"vuera": "^0.2.4",
		"webpack": "^4.29.6",
		"yorkie": "^2.0.0"
	},
	"engines": {
		"node": ">=10.x.x",
		"npm": ">=6.x.x"
	}
}
