		"axios": "^1.7.4",
		"canvas": "^3.1.0",
		"dayjs": "^1.11.10",
		"kjua": "^0.10.0",
		"maska": "^2.1.10",
		"mobile-drag-drop": "^3.0.0-rc.0",
		"object-hash": "^3.0.0",
		"pinia": "^3.0.0",
		"socket.io-client": "^4.7.5",
		"sortablejs": "^1.15.2",
		"sortablejs-vue3": "^1.2.11",
		"vue": "^3.4.21",
		"vue-dompurify-html": "^4.1.4",
		"vue-i18n": "^9.14.2",
		"vue-router": "^4.5.0",
