{
	"name": "schulcloud-nuxt",
	"version": "22.0.0",
	"description": "The next Level of Schul-Cloud",
	"author": "HPI Schul-Cloud",
	"private": true,
	"scripts": {
		"dev:nuxt": "nuxt",
		"dev:storybook": "start-storybook -c stories/.storybook -p 4001 --quiet",
		"dev:docs": "vuepress dev docs",
		"dev": "concurrently \"npm run dev:nuxt\" \"npm run dev:storybook\" \"npm run dev:docs\" -n \"nuxt,story,docs\" -c \"green,magenta,cyan\" -i false --kill-others-on-fail",
		"build:nuxt": "cross-env NODE_ENV=production nuxt build",
		"build:docs": "vuepress build docs -d dist/docs",
		"build:storybook": "build-storybook -c stories/.storybook -o dist/storybook --quiet",
		"build": "run-s build:*",
		"start:nuxt": "nuxt start",
		"start:storybook": "npx http-server dist/storybook -p 4001 -s",
		"start": "run-p start:*",
		"new": "hygen new",
		"lint:eslint": "eslint",
		"lint:stylelint": "stylelint",
		"lint:markdownlint": "markdownlint -i \"src/legacy-client\" -c \".markdownlint.yml\"",
		"lint:prettier": "prettier --loglevel log",
		"lint:all:eslint": "npm run lint:eslint -- --ext .js,.vue .",
		"lint:all:stylelint": "npm run lint:stylelint -- \"**/*.{vue,sass,scss,css}\"",
		"lint:all:prettier": "npm run lint:prettier -- -c \"**/*.{js,json,css,scss,vue,html,md}\"",
		"lint:all:markdownlint": "npm run lint:markdownlint -- \"@(src|docs|stories)/**/*.md\"",
		"lint:fix:all:eslint": "npm run lint:all:eslint -- --fix",
		"lint:fix:all:stylelint": "npm run lint:all:stylelint -- --fix",
		"lint:fix:all:prettier": "npm run lint:all:prettier -- --write",
		"lint:fix:all:markdownlint": "npm run lint:all:markdownlint",
		"lint:ci": "run-s lint:all:*",
		"lint": "run-s lint:fix:all:*",
		"test:unit": "cross-env NODE_ENV=test jest",
		"test:unit:ci": "npm run test:unit -- --coverage --ci",
		"test:unit:file": "npm run test:unit -- --bail --findRelatedTests",
		"test:unit:watch": "npm run test:unit -- --watch --notify --coverage",
		"test:screenshot:jest": "cross-env NODE_ENV=test jest --config tests/screenshot/jest.config.js",
		"test:screenshot": "start-server-and-test start:storybook http-get://localhost:4001 test:screenshot:jest",
		"test:ci": "npm run test:unit:ci",
		"test": "npm run test:unit"
	},
	"gitHooks": {
		"pre-commit": "cross-env PRE_COMMIT=true lint-staged"
	},
	"dependencies": {
		"@nuxtjs/axios": "^5.9.5",
		"@nuxtjs/sentry": "^3.2.4",
		"@nuxtjs/toast": "^3.3.0",
		"@tryghost/content-api": "^1.3.7",
		"cookie-universal-nuxt": "^2.1.2",
		"dayjs": "^1.8.20",
		"echarts": "^4.4.0",
		"font-awesome": "^4.7.0",
		"git-rev-sync": "^2.0.0",
		"glob": "^7.1.6",
		"jwt-decode": "^2.2.0",
		"kjua": "^0.6.0",
		"material-icons-svg": "^1.0.0",
		"nuxt": "^2.11.0",
		"tiptap": "^1.26.6",
		"tiptap-extensions": "^1.28.6",
		"vue": "^2.6.11",
		"vue-echarts": "^4.0.4",
		"vue-flatpickr-component": "^8.1.5",
		"vue-i18n": "^8.15.3",
		"vue-loader": "^15.9.0",
		"vue-mq": "^1.0.1",
		"vue-multiselect": "^2.1.6",
		"vue-ripple-directive": "^2.0.1",
		"vue-router": "^3.1.5",
		"vue-swatches": "^1.0.4"
	},
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"@babel/plugin-proposal-optional-chaining": "^7.8.3",
		"@babel/preset-env": "^7.8.4",
		"@nuxtjs/dotenv": "^1.4.1",
		"@schul-cloud/eslint-config": "^0.4.0",
		"@schul-cloud/prettier-config": "^0.1.5",
		"@schul-cloud/stylelint-config": "^0.1.5",
		"@storybook/addon-a11y": "^5.3.13",
		"@storybook/addon-actions": "^5.3.13",
		"@storybook/addon-centered": "^5.3.13",
		"@storybook/addon-knobs": "^5.3.13",
		"@storybook/addon-links": "^5.3.13",
		"@storybook/addon-notes": "^5.3.13",
		"@storybook/addon-options": "^5.3.13",
		"@storybook/addon-viewport": "^5.3.13",
		"@storybook/addons": "^5.3.13",
		"@storybook/vue": "^5.3.13",
		"@vue/test-utils": "^1.0.0-beta.31",
		"@vuepress/plugin-pwa": "^1.3.1",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^25.1.0",
		"babel-loader": "^8.0.6",
		"babel-plugin-dynamic-import-node": "^2.3.0",
		"babel-preset-vue": "^2.0.2",
		"concurrently": "^5.1.0",
		"core-js": "^3.6.4",
		"cross-env": "^7.0.0",
		"css-loader": "^3.4.2",
		"eslint": "^6.8.0",
		"eslint-loader": "^3.0.3",
		"faker": "^4.1.0",
		"find-cache-dir": "^3.2.0",
		"flush-promises": "^1.0.2",
		"http-proxy-middleware": "^1.0.0",
		"http-server": "^0.12.1",
		"hygen": "^5.0.3",
		"imagemin-lint-staged": "^0.4.0",
		"jest": "^24.1.0",
		"jest-canvas-mock": "^2.2.0",
		"jest-haste-map": "^25.1.0",
		"jest-image-snapshot": "^2.12.0",
		"jest-puppeteer": "^4.4.0",
		"jest-resolve": "^25.1.0",
		"jest-serializer-vue": "^2.0.2",
		"lint-staged": "^10.0.7",
		"lodash": "^4.17.15",
		"markdownlint-cli": "^0.22.0",
		"node-sass": "^4.13.1",
		"npm-run-all": "^4.1.5",
		"nuxt-babel": "^1.0.1",
		"postcss-color-mod-function": "^3.0.3",
		"postcss-loader": "^3.0.0",
		"prettier": "^1.19.1",
		"puppeteer": "^2.1.1",
		"sass-loader": "^8.0.2",
		"sinon": "^8.1.1",
		"start-server-and-test": "^1.10.8",
		"storybook-addon-vue-info": "^1.4.1",
		"style-loader": "^1.1.3",
		"stylelint": "^13.2.0",
		"stylelint-declaration-use-variable": "^1.7.2",
		"vue-jest": "^3.0.5",
		"vue-style-loader": "^4.1.2",
		"vue-svg-loader": "0.15.0",
		"vue-template-compiler": "^2.6.11",
		"vuepress": "^1.3.0",
		"yorkie": "^2.0.0"
	},
	"engines": {
		"node": ">=10.x.x",
		"npm": ">=6.x.x"
	}
}
