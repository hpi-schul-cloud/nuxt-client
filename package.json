{
	"name": "schulcloud-nuxt",
	"version": "1.0.1",
	"description": "The next Level of Schul-Cloud",
	"author": "HPI Schul-Cloud",
	"private": true,
	"scripts": {
		"dev:nuxt": "npx nodemon --inspect --watch src/ --ignore src/legacy-client src/server",
		"dev:storybook": "start-storybook -c stories/.storybook -p 4001",
		"dev:docs": "vuepress dev docs",
		"dev": "concurrently \"npm run dev:nuxt\" \"npm run dev:storybook\" \"npm run dev:docs\" -n \"nuxt,story,docs\" -c \"green,magenta,cyan\" -i false --kill-others-on-fail",
		"build:legacy": "cd src/legacy-client && npx gulp",
		"build:nuxt": "cross-env NODE_ENV=production nuxt build && cross-env sh ./copy_static.sh",
		"build:docs": "vuepress build docs -d dist/docs",
		"build:storybook": "build-storybook -c stories/.storybook -o dist/storybook",
		"build": "run-s build:*",
		"start": "node src/server",
		"setup": "git submodule init && git submodule update && cd src/legacy-client && npm i && npx gulp",
		"new": "hygen new",
		"lint:eslint": "eslint",
		"lint:stylelint": "stylelint",
		"lint:markdownlint": "markdownlint -i \"src/legacy-client\" -c \".markdownlint.yml\"",
		"lint:prettier": "prettier --loglevel log",
		"lint:all:eslint": "npm run lint:eslint -- --ext .js,.vue .",
		"lint:all:stylelint": "npm run lint:stylelint -- \"**/*.{vue,sass,scss,css}\"",
		"lint:all:prettier": "npm run lint:prettier -- -c \"**/*.{js,json,css,scss,vue,html,md}\"",
		"lint:all:markdownlint": "npm run lint:markdownlint -- \"@(src|docs|stories)/**/*.md\"",
		"lint:fix:all:eslint": "npm run lint:all:eslint -- --fix",
		"lint:fix:all:stylelint": "npm run lint:all:stylelint -- --fix",
		"lint:fix:all:prettier": "npm run lint:all:prettier -- --write",
		"lint:fix:all:markdownlint": "npm run lint:all:markdownlint",
		"lint:ci": "run-s lint:all:*",
		"lint": "run-s lint:fix:all:*",
		"test:unit": "cross-env NODE_ENV=test jest",
		"test:unit:ci": "npm run test:unit -- --coverage --ci",
		"test:unit:file": "npm run test:unit -- --bail --findRelatedTests",
		"test:unit:watch": "npm run test:unit -- --watch --notify",
		"test:ci": "npm run test:unit:ci",
		"test": "npm run test:unit"
	},
	"gitHooks": {
		"pre-commit": "cross-env PRE_COMMIT=true lint-staged"
	},
	"dependencies": {
		"@feathersjs/authentication-client": "^1.0.8",
		"@feathersjs/feathers": "^3.2.3",
		"@feathersjs/socketio-client": "^1.1.5",
		"@fortawesome/free-solid-svg-icons": "^5.7.2",
		"@nuxtjs/axios": "^5.5.4",
		"@nuxtjs/toast": "^3.0.2",
		"consola": "^2.6.0",
		"cookie-parser": "^1.4.4",
		"cookie-storage": "^5.0.0",
		"cookie-universal-nuxt": "^2.0.16",
		"dayjs": "^1.8.14",
		"express": "^4.16.4",
		"express-session": "^1.16.1",
		"feathers-vuex": "1.7.0",
		"handlebars": "^4.1.2",
		"handlebars-layouts": "^3.1.4",
		"handlebars-wax": "^6.1.0",
		"jwt-decode": "^2.2.0",
		"lodash": "^4.17.11",
		"material-icons-svg": "^1.0.0",
		"nuxt": "^2.6.3",
		"socket.io-client": "^2.2.0",
		"vue": "^2.6.10",
		"vue-flatpickr-component": "^8.1.1",
		"vue-i18n": "^8.11.2",
		"vue-loader": "^15.6.2",
		"vue-multiselect": "^2.1.6",
		"vue-ripple-directive": "^2.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"@nuxtjs/dotenv": "^1.3.0",
		"@storybook/addon-a11y": "^5.0.3",
		"@storybook/addon-actions": "^5.0.3",
		"@storybook/addon-centered": "^5.0.3",
		"@storybook/addon-knobs": "^5.0.3",
		"@storybook/addon-links": "^5.0.3",
		"@storybook/addon-notes": "^5.0.3",
		"@storybook/addon-viewport": "^5.0.3",
		"@storybook/addons": "^5.0.3",
		"@storybook/vue": "^5.0.3",
		"@vue/cli-plugin-babel": "^3.3.0",
		"@vue/cli-service": "^3.3.1",
		"@vue/eslint-config-prettier": "^5.0.0",
		"@vue/eslint-config-standard": "^4.0.0",
		"@vue/test-utils": "^1.0.0-beta.28",
		"@vuepress/plugin-pwa": "^1.0.2",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^24.1.0",
		"babel-loader": "^8.0.5",
		"babel-plugin-dynamic-import-node": "^2.2.0",
		"babel-preset-vue": "^2.0.2",
		"concurrently": "^4.1.0",
		"core-js": "2.6.5",
		"cross-env": "^5.2.0",
		"css-loader": "^2.1.1",
		"eslint": "^6.0.1",
		"eslint-config-prettier": "^6.0.0",
		"eslint-loader": "^2.2.1",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-vue": "^5.2.1",
		"find-cache-dir": "^3.0.0",
		"hygen": "^4.0.1",
		"imagemin-lint-staged": "^0.4.0",
		"jest": "^24.1.0",
		"jest-haste-map": "^24.5.0",
		"jest-resolve": "^24.5.0",
		"jest-serializer-vue": "^2.0.2",
		"lint-staged": "^9.0.0",
		"markdownlint-cli": "github:igorshubovych/markdownlint-cli",
		"node-sass": "^4.11.0",
		"nodemon": "^1.18.10",
		"npm-run-all": "^4.1.5",
		"nuxt-babel": "^1.0.1",
		"postcss-color-mod-function": "^3.0.3",
		"postcss-loader": "^3.0.0",
		"prettier": "^1.18.2",
		"react": "^16.8.4",
		"react-dom": "^16.8.4",
		"sass-loader": "^7.1.0",
		"storybook-addon-vue-info": "^1.1.1",
		"style-loader": "^0.23.1",
		"stylelint": "^10.0.0",
		"stylelint-config-prettier": "^5.0.0",
		"stylelint-config-recess-order": "^2.0.1",
		"stylelint-config-standard": "^18.2.0",
