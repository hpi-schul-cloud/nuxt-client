{
	"name": "schulcloud-nuxt",
	"description": "The next Level of Dataport Bildungscloud",
	"author": "Dataport Bildungscloud",
	"private": true,
	"type": "module",
	"scripts": {
		"serve": "vite --host",
		"build": "run-p type-check \"build-only {@}\" --",
		"build-only": "vite build",
		"type-check": "vue-tsc --build --force",
		"test": "npm run test:unit",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest watch",
		"test:unit:ci": "vitest run --coverage",
		"lint": "npx eslint 'src/**/*.{ts,js,vue}'",
		"lint:fix": "npx eslint 'src/**/*.{ts,js,vue}' --fix",
		"generate-client:server": "openapi-generator-cli generate -c ./openapitools.json --generator-key server-api",
		"generate-client:filestorage": "openapi-generator-cli generate -c ./openapitools.json --generator-key file-storage-api",
		"generate-client:h5p-editor": "openapi-generator-cli generate -c ./openapitools.json --generator-key h5p-editor-api",
		"generate-client:common-cartridge": "openapi-generator-cli generate -c ./openapitools.json --generator-key common-cartridge-api"
	},
	"dependencies": {
		"@braintree/sanitize-url": "^7.1.1",
		"@ckeditor/ckeditor5-core": "^37.1.0",
		"@ckeditor/ckeditor5-vue": "^5.1.0",
		"@hpi-schul-cloud/ckeditor": "^2.1.0",
		"@lumieducation/h5p-webcomponents": "^10.0.4",
		"@vuelidate/core": "^2.0.3",
		"@vuelidate/validators": "^2.0.4",
		"@vueuse/components": "^13.3.0",
		"@vueuse/core": "^13.3.0",
		"@vueuse/integrations": "^13.3.0",
		"axios": "^1.8.2",
		"canvas": "^3.1.0",
		"dayjs": "^1.11.10",
		"focus-trap": "^7.6.5",
		"katex": "^0.16.21",
		"kjua": "^0.10.0",
		"maska": "^2.1.10",
		"mobile-drag-drop": "^3.0.0-rc.0",
		"object-hash": "^3.0.0",
		"pinia": "^3.0.2",
		"socket.io-client": "^4.8.1",
		"sortablejs": "^1.15.6",
		"sortablejs-vue3": "^1.2.11",
		"vue": "^3.5.14",
		"vue-dompurify-html": "^4.1.4",
		"vue-i18n": "^11.1.10",
		"vue-router": "^4.5.1",
		"vuedraggable": "^4.1.0",
		"vuetify": "^3.8.6",
		"vuex": "^4.0.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.27.0",
		"@golevelup/ts-vitest": "^0.5.2",
		"@intlify/unplugin-vue-i18n": "^6.0.8",
		"@mdi/js": "^7.2.96",
		"@openapitools/openapi-generator-cli": "^2.19.1",
		"@pinia/testing": "^1.0.1",
		"@types/katex": "^0.16.7",
		"@types/lodash": "^4.17.17",
		"@types/object-hash": "^3.0.6",
		"@types/sortablejs": "^1.15.7",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vitest/coverage-v8": "^3.2.4",
		"@vue/eslint-config-typescript": "^14.5.0",
		"@vue/test-utils": "^2.4.5",
		"arg": "^5.0.2",
		"connect": "^3.7.0",
		"eslint": "^9.27.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.4.0",
		"eslint-plugin-vue": "^10.2.0",
		"fishery": "^2.3.1",
		"globals": "^15.9.0",
		"http-proxy-middleware": "^3.0.5",
		"jsdom": "^26.1.0",
		"npm-run-all2": "^7.0.2",
		"prettier": "^3.3.3",
		"resize-observer-polyfill": "^1.5.1",
		"sass-embedded": "^1.86.3",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.33.1",
		"vite": "^7.0.6",
		"vite-plugin-checker": "^0.9.3",
		"vite-plugin-vue-devtools": "^8.0.0",
		"vite-plugin-vuetify": "^2.1.1",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4",
		"vue-tsc": "^2.2.8",
		"vuex-module-decorators": "^1.2.0"
	},
	"overrides": {
		"@openapitools/openapi-generator-cli": {
			"axios": "$axios"
		},
		"canvas": "^3.1.0"
	},
	"engines": {
		"node": "22",
		"npm": ">=9"
	},
	"optionalDependencies": {
		"@rollup/rollup-darwin-arm64": "^4.45.1",
		"@rollup/rollup-linux-arm64-gnu": "^4.45.1",
		"@rollup/rollup-linux-x64-gnu": "^4.45.1",
		"@rollup/rollup-linux-x64-musl": "^4.45.1",
		"@rollup/rollup-win32-x64": "^4.45.1",
		"sass-embedded-darwin-arm64": "^1.89.2",
		"sass-embedded-linux-arm64": "^1.89.2",
		"sass-embedded-linux-musl-x64": "^1.89.2",
		"sass-embedded-linux-x64": "^1.89.2",
		"sass-embedded-win32-x64": "^1.89.2"
	}
}
