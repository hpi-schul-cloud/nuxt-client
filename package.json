{
	"name": "schulcloud-nuxt",
	"description": "The next Level of Dataport Bildungscloud",
	"author": "Dataport Bildungscloud",
	"private": true,
	"type": "module",
	"scripts": {
		"serve": "vite --host",
		"build": "run-p type-check \"build-only {@}\" --",
		"build-only": "vite build",
		"type-check": "vue-tsc --build --force",
		"test": "npm run test:unit",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest watch",
		"test:unit:ci": "vitest run --coverage",
		"lint": "npx eslint src/**/*.{ts,js,vue}",
		"lint:fix": "npx eslint src/**/*.{ts,js,vue} --fix",
		"generate-client:server": "openapi-generator-cli generate -c ./openapitools.json --generator-key server-api",
		"generate-client:filestorage": "openapi-generator-cli generate -c ./openapitools.json --generator-key file-storage-api",
		"generate-client:h5p-editor": "openapi-generator-cli generate -c ./openapitools.json --generator-key h5p-editor-api",
		"generate-client:common-cartridge": "openapi-generator-cli generate -c ./openapitools.json --generator-key common-cartridge-api"
	},
	"dependencies": {
		"@braintree/sanitize-url": "^7.1.1",
		"@ckeditor/ckeditor5-core": "^41.4.2",
		"@ckeditor/ckeditor5-vue": "^5.1.0",
		"@hpi-schul-cloud/ckeditor": "^2.1.1",
		"@lumieducation/h5p-webcomponents": "^10.0.4",
		"@types/lodash-es": "^4.17.12",
		"@vuelidate/core": "^2.0.3",
		"@vuelidate/validators": "^2.0.4",
		"@vueuse/components": "^13.3.0",
		"@vueuse/core": "^13.3.0",
		"@vueuse/integrations": "^13.3.0",
		"axios": "^1.12.2",
		"dayjs": "^1.11.18",
		"focus-trap": "^7.6.5",
		"katex": "^0.16.21",
		"lodash-es": "^4.17.21",
		"maska": "^2.1.10",
		"mobile-drag-drop": "^3.0.0-rc.0",
		"object-hash": "^3.0.0",
		"pinia": "^3.0.3",
		"qrcode.vue": "^3.6.0",
		"socket.io-client": "^4.8.1",
		"sortablejs": "^1.15.6",
		"sortablejs-vue3": "^1.3.0",
		"vue": "^3.5.22",
		"vue-dompurify-html": "^4.1.4",
		"vue-i18n": "^11.1.12",
		"vue-router": "^4.6.3",
		"vuedraggable": "^4.1.0",
		"vuetify": "^3.10.7",
		"vuex": "^4.1.0",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@eslint/js": "^9.38.0",
		"@golevelup/ts-vitest": "^0.5.2",
		"@intlify/unplugin-vue-i18n": "^6.0.8",
		"@mdi/js": "^7.4.47",
		"@openapitools/openapi-generator-cli": "^2.25.0",
		"@pinia/testing": "^1.0.2",
		"@types/katex": "^0.16.7",
		"@types/object-hash": "^3.0.6",
		"@types/sortablejs": "^1.15.9",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vitest/coverage-v8": "^3.2.4",
		"@vue/eslint-config-typescript": "^14.6.0",
		"@vue/test-utils": "^2.4.6",
		"arg": "^5.0.2",
		"connect": "^3.7.0",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-vue": "^10.5.1",
		"fishery": "^2.3.1",
		"globals": "^15.9.0",
		"http-proxy-middleware": "^3.0.5",
		"jsdom": "^26.1.0",
		"npm-run-all2": "^7.0.2",
		"prettier": "^3.6.2",
		"resize-observer-polyfill": "^1.5.1",
		"sass-embedded": "^1.93.2",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.46.2",
		"vite": "^7.1.12",
		"vite-plugin-checker": "^0.9.3",
		"vite-plugin-vue-devtools": "^8.0.3",
		"vite-plugin-vuetify": "^2.1.2",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4",
		"vue-tsc": "^2.2.8",
		"vuex-module-decorators": "^1.2.0"
	},
	"overrides": {
		"@openapitools/openapi-generator-cli": {
			"axios": "$axios"
		}
	},
	"engines": {
		"node": "22",
		"npm": ">=9"
	}
}
