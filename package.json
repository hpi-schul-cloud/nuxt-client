{
	"name": "schulcloud-nuxt",
	"description": "The next Level of Dataport Bildungscloud",
	"author": "Dataport Bildungscloud",
	"private": true,
	"type": "module",
	"scripts": {
		"serve": "vite --host",
		"build": "run-p type-check \"build-only {@}\" --",
		"build-only": "vite build",
		"type-check": "vue-tsc --build --force",
		"test": "npm run test:unit",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest watch",
		"test:unit:ci": "vitest run --coverage",
		"lint": "npx eslint src/**/*.{ts,js,vue}",
		"lint:fix": "npx eslint src/**/*.{ts,js,vue} --fix",
		"generate-client:server": "openapi-generator-cli generate -c ./openapitools.json --generator-key server-api",
		"generate-client:fwu": "openapi-generator-cli generate -c ./openapitools.json --generator-key fwu-api",
		"generate-client:filestorage": "openapi-generator-cli generate -c ./openapitools.json --generator-key file-storage-api",
		"generate-client:h5p-editor": "openapi-generator-cli generate -c ./openapitools.json --generator-key h5p-editor-api",
		"generate-client:common-cartridge": "openapi-generator-cli generate -c ./openapitools.json --generator-key common-cartridge-api"
	},
	"dependencies": {
		"@braintree/sanitize-url": "^7.1.2",
		"@ckeditor/ckeditor5-core": "^41.4.2",
		"@ckeditor/ckeditor5-vue": "^5.1.0",
		"@hpi-schul-cloud/ckeditor": "^2.1.1",
		"@lumieducation/h5p-webcomponents": "^10.0.4",
		"@types/lodash-es": "^4.17.12",
		"@vuelidate/core": "^2.0.3",
		"@vuelidate/validators": "^2.0.4",
		"@vueuse/components": "^14.2.1",
		"@vueuse/core": "^14.2.1",
		"@vueuse/integrations": "^14.2.1",
		"axios": "^1.13.5",
		"dayjs": "^1.11.19",
		"focus-trap": "^8.0.0",
		"katex": "^0.16.21",
		"lodash-es": "^4.17.23",
		"maska": "^3.2.0",
		"mobile-drag-drop": "^3.0.0-rc.0",
		"object-hash": "^3.0.0",
		"pinia": "^3.0.4",
		"qrcode.vue": "^3.8.0",
		"socket.io-client": "^4.8.3",
		"sortablejs": "^1.15.7",
		"sortablejs-vue3": "^1.3.0",
		"vue": "^3.5.28",
		"vue-dompurify-html": "^5.3.0",
		"vue-i18n": "^11.2.8",
		"vue-router": "^4.6.3",
		"vuedraggable": "^4.1.0",
		"vuetify": "^3.11.8",
		"vuex": "^4.1.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@golevelup/ts-vitest": "^0.5.2",
		"@mdi/js": "^7.4.47",
		"@openapitools/openapi-generator-cli": "^2.29.0",
		"@pinia/testing": "^1.0.3",
		"@types/katex": "^0.16.8",
		"@types/object-hash": "^3.0.6",
		"@types/sortablejs": "^1.15.9",
		"@vitejs/plugin-vue": "^6.0.4",
		"@vitest/coverage-v8": "^4.0.18",
		"@vue/eslint-config-typescript": "^14.7.0",
		"@vue/test-utils": "^2.4.6",
		"arg": "^5.0.2",
		"connect": "^3.7.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^9.1.2",
		"eslint-plugin-prettier": "^5.5.5",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-unused-imports": "^4.4.1",
		"eslint-plugin-vue": "^10.8.0",
		"fishery": "^2.4.0",
		"globals": "^17.3.0",
		"http-proxy-middleware": "^3.0.5",
		"jsdom": "^28.1.0",
		"npm-run-all2": "^7.0.2",
		"prettier": "^3.8.1",
		"resize-observer-polyfill": "^1.5.1",
		"sass-embedded": "^1.97.3",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0",
		"vite": "^7.3.1",
		"vite-plugin-checker": "^0.12.0",
		"vite-plugin-vue-devtools": "^8.0.6",
		"vite-plugin-vuetify": "^2.1.3",
		"vitest": "^4.0.18",
		"vue-router-mock": "^2.0.0",
		"vue-tsc": "^3.2.4",
		"vuex-module-decorators": "^1.2.0"
	},
	"overrides": {
		"@openapitools/openapi-generator-cli": {
			"axios": "$axios"
		}
	},
	"engines": {
		"node": "22",
		"npm": ">=9"
	}
}
