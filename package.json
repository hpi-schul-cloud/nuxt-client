{
	"name": "schulcloud-nuxt",
	"description": "The next Level of Dataport Bildungscloud",
	"author": "Dataport Bildungscloud",
	"private": true,
	"scripts": {
		"dev:nuxt": "nuxt",
		"build:nuxt": "nuxt build",
		"build": "run-s build:*",
		"start:nuxt": "nuxt start",
		"start": "run-p start:*",
		"new": "hygen new",
		"lint:eslint": "eslint",
		"lint:stylelint": "stylelint",
		"lint:markdownlint": "markdownlint -c \".markdownlint.yml\"",
		"lint:prettier": "prettier --loglevel log",
		"lint:all:eslint": "npm run lint:eslint -- --ext .js,.vue .",
		"lint:all:stylelint": "npm run lint:stylelint -- \"**/*.{vue,sass,scss,css}\"",
		"lint:all:prettier": "npm run lint:prettier -- -c \"**/*.{js,json,css,scss,vue,html,md}\"",
		"lint:all:markdownlint": "npm run lint:markdownlint -- \"@(src|docs)/**/*.md\"",
		"lint:fix:all:eslint": "npm run lint:all:eslint -- --fix",
		"lint:fix:all:stylelint": "npm run lint:all:stylelint -- --fix",
		"lint:fix:all:prettier": "npm run lint:all:prettier -- --write",
		"lint:fix:all:markdownlint": "npm run lint:all:markdownlint",
		"lint:ci": "run-s lint:all:*",
		"lint": "run-s lint:fix:all:*",
		"test:unit": "jest",
		"test:unit:ci": "npm run test:unit -- --coverage --ci",
		"test:unit:file": "npm run test:unit -- --bail --findRelatedTests",
		"test:unit:watch": "npm run test:unit -- --watch --notify --coverage",
		"test:ci": "run-s test:*:ci",
		"test": "npm run test:unit",
		"generate-client:server": "node generate-client.js",
		"generate-client:filestorage": "npm run generate-client:server -u 'http://localhost:4444/api/v3/docs-json/' -p 'src/fileStorageApi/v3'",
		"vue:serve": "vue-cli-service serve"
	},
	"gitHooks": {
		"pre-commit": "npx lint-staged"
	},
	"dependencies": {
		"@mdi/js": "^5.9.55",
		"@openapitools/openapi-generator-cli": "^2.4.5",
		"@tryghost/content-api": "^1.4.11",
		"dayjs": "^1.9.7",
		"echarts": "^4.9.0",
		"font-awesome": "^4.7.0",
		"glob": "^7.1.6",
		"html-entities": "^1.3.3",
		"kjua": "^0.9.0",
		"material-icons-svg": "^1.0.0",
		"mobile-drag-drop": "^2.3.0-rc.2",
		"moment": "^2.29.1",
		"tiptap": "^1.32.2",
		"tiptap-extensions": "^1.34.0",
		"vue": "^2.6.14",
		"vue-echarts": "^4.0.4",
		"vue-filter-ui": "^0.8.0",
		"vue-flatpickr-component": "^8.1.6",
		"vue-i18n": "^8.24.3",
		"vue-multiselect": "^2.1.6",
		"vue-router": "^3.5.1",
		"vuedraggable": "^2.24.3",
		"vuelidate": "^0.7.6",
		"vuetify": "^2.6.0",
		"vuex": "^3.6.2"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.11.10",
		"@types/jest": "^27.0.1",
		"@types/object-hash": "^2.1.1",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"@vue/cli-plugin-eslint": "~5.0.0",
		"@vue/cli-plugin-router": "~5.0.0",
		"@vue/cli-plugin-typescript": "~5.0.0",
		"@vue/cli-plugin-unit-jest": "~5.0.0",
		"@vue/cli-plugin-vuex": "~5.0.0",
		"@vue/cli-service": "~5.0.0",
		"@vue/eslint-config-typescript": "^9.1.0",
		"@vue/test-utils": "^1.1.3",
		"babel-jest": "^27.0.6",
		"concurrently": "^6.2.1",
		"cross-env": "^7.0.3",
		"deepmerge": "^4.2.2",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^8.0.3",
		"faker": "^5.5.3",
		"find-cache-dir": "^3.3.1",
		"flush-promises": "^1.0.2",
		"http-proxy-middleware": "^1.0.6",
		"jest": "^27.0.5",
		"lodash": "^4.17.21",
		"markdownlint-cli": "^0.28.1",
		"npm-run-all": "^4.1.5",
		"postcss-color-mod-function": "^3.0.3",
		"postcss-loader": "^3.0.0",
		"prettier": "^2.7.1",
		"sass": "^1.32.7",
		"sass-loader": "^12.0.0",
		"style-loader": "^2.0.0",
		"stylelint": "^13.12.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-recess-order": "^2.2.0",
		"stylelint-config-standard": "^22.0.0",
		"stylelint-declaration-use-variable": "^1.7.3",
		"stylelint-scss": "^3.20.0",
		"svg-inline-loader": "^0.8.2",
		"ts-jest": "^27.0.4",
		"vuetify-loader": "^1.7.0",
		"vuex-module-decorators": "^1.2.0",
		"yorkie": "^2.0.0"
	},
	"engines": {
		"node": "16",
		"npm": "8"
	}
}
