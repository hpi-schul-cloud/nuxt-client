{
	"name": "schulcloud-nuxt",
	"version": "1.0.0",
	"description": "The next Level of Schul-Cloud",
	"author": "HPI Schul-Cloud",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_ENV=development nodemon src/server/index.js --watch server",
		"docs": "vuepress dev docs",
		"storybook": "start-storybook -c stories/.storybook -p 6006",
		"new": "hygen new",
		"build": "run-s build:*",
		"build:nuxt": "cross-env NODE_ENV=production nuxt generate",
		"build:docs": "vuepress build docs",
		"build:storybook": "build-storybook -c stories/.storybook",
		"lint:eslint": "eslint --fix",
		"lint:stylelint": "stylelint --fix",
		"lint:markdownlint": "markdownlint \"*/*.md\" -i \"schulcloud-client\"",
		"lint:prettier": "prettier --write --loglevel warn",
		"lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
		"lint:all:stylelint": "yarn lint:stylelint \"**/*.{vue,sass,scss,css}\"",
		"lint:all:markdownlint": "yarn lint:markdownlint",
		"lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
		"lint": "run-s lint:all:*",
		"test:unit": "cross-env NODE_ENV=test jest",
		"test:unit:ci": "yarn test:unit --coverage --ci",
		"test:unit:file": "yarn test:unit --bail --findRelatedTests",
		"test:unit:watch": "yarn test:unit --watch --notify",
		"test:ci": "yarn test:unit:ci",
		"test": "yarn test:unit",
		"ci": "yarn test:ci && yarn lint && yarn build"
	},
	"gitHooks": {
		"pre-commit": "cross-env PRE_COMMIT=true lint-staged"
	},
	"dependencies": {
		"@feathersjs/authentication-client": "^1.0.8",
		"@feathersjs/feathers": "^3.2.3",
		"@feathersjs/socketio-client": "^1.1.5",
		"@fortawesome/fontawesome-svg-core": "^1.2.13",
		"@fortawesome/free-regular-svg-icons": "^5.7.1",
		"@fortawesome/free-solid-svg-icons": "^5.7.0",
		"@fortawesome/vue-fontawesome": "^0.1.5",
		"@nuxtjs/axios": "^5.0.0",
		"@storybook/addon-a11y": "^4.1.11",
		"@storybook/addon-centered": "^4.1.11",
		"@storybook/addon-viewport": "^4.1.11",
		"cookie-storage": "^3.2.0",
		"feathers-vuex": "^1.6.2",
		"handlebars": "^4.0.12",
		"handlebars-layouts": "^3.1.4",
		"handlebars-wax": "^6.1.0",
		"lodash": "^4.17.11",
		"moment": "^2.23.0",
		"nuxt": "^2.4.2",
		"nuxt-express-module": "0.0.11",
		"socket.io-client": "^2.2.0",
		"storybook-addon-vue-info": "^0.7.1",
		"style-loader": "^0.23.1",
		"stylelint-declaration-use-variable": "^1.7.0",
		"vue-loader": "^15.6.2",
		"vue-ripple-directive": "^2.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/preset-env": "^7.3.1",
		"@storybook/addon-actions": "^4.1.11",
		"@storybook/addon-links": "^4.1.11",
		"@storybook/addon-notes": "^4.1.11",
		"@storybook/addons": "^4.1.11",
		"@storybook/vue": "^4.1.11",
		"@vue/cli-plugin-babel": "^3.3.0",
		"@vue/cli-service": "^3.3.1",
		"@vue/eslint-config-prettier": "^4.0.1",
		"@vue/eslint-config-standard": "^4.0.0",
		"@vue/test-utils": "^1.0.0-beta.28",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^24.1.0",
		"babel-preset-vue": "^2.0.2",
		"cross-env": "^5.2.0",
		"css-loader": "^2.1.0",
		"eslint": "^5.12.1",
		"eslint-config-prettier": "^4.0.0",
		"eslint-loader": "^2.0.0",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-vue": "^5.2.1",
		"hygen": "^2.1.1",
		"imagemin-lint-staged": "^0.4.0",
		"jest": "^24.1.0",
		"jest-serializer-vue": "^2.0.2",
		"lint-staged": "^8.1.1",
		"markdownlint-cli": "github:igorshubovych/markdownlint-cli",
		"node-sass": "^4.11.0",
		"nodemon": "^1.18.10",
		"npm-run-all": "^4.1.5",
		"nuxt-babel": "^0.1.2",
		"prettier": "github:prettier/prettier#3de36e3",
		"sass-loader": "^7.1.0",
		"stylelint": "^9.10.1",
		"stylelint-config-prettier": "^4.0.0",
		"stylelint-config-recess-order": "^2.0.1",
		"stylelint-config-standard": "^18.2.0",
		"stylelint-scss": "^3.5.1",
		"vue-jest": "^3.0.3",
		"vue-style-loader": "^4.1.2",
		"vue-svg-loader": "^0.11.0",
		"vuepress": "^0.14.8",
		"yorkie": "^2.0.0"
	}
}
