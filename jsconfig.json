{
	"baseUrl": ".",
	"include": ["src/**/*"],
	"compilerOptions": {
		"baseUrl": ".",
		"target": "esnext",
		"module": "es2015",
		"paths": {
			"~/*": ["./*"],
			"~": [
				"./index.js",
				"./index.json",
				"./index.vue",
				"./index.scss",
				"./index.css"
			],
			"@/*": ["src/*"],
			"@": [
				"src/index.js",
				"src/index.json",
				"src/index.vue",
				"src/index.scss",
				"src/index.css"
			],
			"@assets/*": ["src/assets/*"],
			"@assets": [
				"src/assets/index.js",
				"src/assets/index.json",
				"src/assets/index.vue",
				"src/assets/index.scss",
				"src/assets/index.css"
			],
			"@components/*": ["src/components/*"],
			"@components": [
				"src/components/index.js",
				"src/components/index.json",
				"src/components/index.vue",
				"src/components/index.scss",
				"src/components/index.css"
			],
			"@docs/*": ["docs/*"],
			"@docs": [
				"docs/index.js",
				"docs/index.json",
				"docs/index.vue",
				"docs/index.scss",
				"docs/index.css"
			],
			"@layouts/*": ["src/layouts/*"],
			"@layouts": [
				"src/layouts/index.js",
				"src/layouts/index.json",
				"src/layouts/index.vue",
				"src/layouts/index.scss",
				"src/layouts/index.css"
			],
			"@middleware/*": ["src/middleware/*"],
			"@middleware": [
				"src/middleware/index.js",
				"src/middleware/index.json",
				"src/middleware/index.vue",
				"src/middleware/index.scss",
				"src/middleware/index.css"
			],
			"@pages/*": ["src/pages/*"],
			"@pages": [
				"src/pages/index.js",
				"src/pages/index.json",
				"src/pages/index.vue",
				"src/pages/index.scss",
				"src/pages/index.css"
			],
			"@plugins/*": ["src/plugins/*"],
			"@plugins": [
				"src/plugins/index.js",
				"src/plugins/index.json",
				"src/plugins/index.vue",
				"src/plugins/index.scss",
				"src/plugins/index.css"
			],
			"@static/*": ["src/static/*"],
			"@static": [
				"src/static/index.js",
				"src/static/index.json",
				"src/static/index.vue",
				"src/static/index.scss",
				"src/static/index.css"
			],
			"@store/*": ["src/store/*"],
			"@store": [
				"src/store/index.js",
				"src/store/index.json",
				"src/store/index.vue",
				"src/store/index.scss",
				"src/store/index.css"
			],
			"@theme/*": ["src/themes/n21/*"],
			"@theme": [
				"src/themes/n21/index.js",
				"src/themes/n21/index.json",
				"src/themes/n21/index.vue",
				"src/themes/n21/index.scss",
				"src/themes/n21/index.css"
			],
			"@styles/*": ["src/themes/n21/styles/*"],
			"@styles": [
				"src/themes/n21/styles/index.js",
				"src/themes/n21/styles/index.json",
				"src/themes/n21/styles/index.vue",
				"src/themes/n21/styles/index.scss",
				"src/themes/n21/styles/index.css"
			],
			"@variables/*": ["src/themes/n21/styles/variables.scss/*"],
			"@variables": [
				"src/themes/n21/styles/variables.scss/index.js",
				"src/themes/n21/styles/variables.scss/index.json",
				"src/themes/n21/styles/variables.scss/index.vue",
				"src/themes/n21/styles/variables.scss/index.scss",
				"src/themes/n21/styles/variables.scss/index.css"
			]
		}
	}
}
